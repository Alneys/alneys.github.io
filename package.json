    "@vue/test-utils": "~2.4.6",
    "@vue/tsconfig": "~0.7.0",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.6.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "~7.0.2",
