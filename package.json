    "pinia": "~3.0.4",
    "vue": "~3.5.26",
    "vue-router": "~4.6.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "~1.12.0",
    "@tsconfig/node24": "^24.0.3",
    "@types/jsdom": "~21.1.7",
    "@types/node": "^24.10.4",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^5.1.3",
    "@vue/eslint-config-prettier": "^10.2.0",
